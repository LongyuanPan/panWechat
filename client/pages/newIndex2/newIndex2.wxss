.new-contain{
  background-color: #f1f1f1;
}
.new-contain swiper{
  width: 100%;
  height: 350rpx;
}
.new-contain swiper image{
  width: 100%;
}
.article-list{
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom:40rpx;
  background-color: #fff;
  border-top: 1px solid #ededed;
  border-bottom: 1px solid #ededed;
  padding: 10px;
  padding-left: 5px
}
.author-time{
  margin-top: 10rpx;
  margin-bottom: 20rpx
}
.authorImg{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle
}
.author{
  margin-left: 20rpx
}
.author-date{
  margin-left: 20rpx;
  vertical-align: middle;
  margin-bottom: 5px;
  font-size: 26rpx
}
.title{
  font-size: 34rpx;
  font-weight: 700;
  color: #333;
  margin-left: 10px;
}
.titleImg{
  margin-left:10px;
  width: 100%;
  height: 340rpx;
  margin: auto 0;
  margin-bottom: 15px;
}
.titleContent{
  color: #666;
  font-size: 26rpx;
  margin-bottom: 20rpx;
  letter-spacing: 2rpx;
  line-height: 40rpx
}
.article-like{
  font-size: 13px;
  flex-direction: row;
  line-height: 16px;
}
.likeImg{
  height: 16px;
  width: 16px;
  margin-right: 8px;
  vertical-align:middle;
}
.like-text{
  vertical-align: middle;
  margin-right: 20px;
}